@using Masa.Blazor
@using Watermark.Shared.Models
@inherits LayoutComponentBase

<div>@Body</div>

@code {
    StringNumber v = 0;
    StringNumber Value
    {
        get => v;
        set
        {
            v = value;
            HapticFeedback.Default.Perform(HapticFeedbackType.Click);
            StateHasChanged();
        }
    }
}